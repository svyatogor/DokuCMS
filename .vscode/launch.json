  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug admin",
      "url": "http://mpfashion.dreamscape.localhost:4000/admin",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "name": "Debug Server",
      "port": 9229,
      "sourceMaps": true,
      "restart": true
    },
  ]
}